{
  "name": "advent-of-code",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "test": "jest",
    "tdd": "jest --watchAll",
    "setup": "node ./setup.js",
    "readme": "node ./update-readme.js",
    "bench": "node ./bench.js",
    "format": "prettier . --write --ignore-unknown"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 160,
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
  },
  "keywords": [],
  "author": "Robin Malfait <malfait.robin@gmail.com>",
  "license": "MIT",
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/templates/"
    ],
    "projects": [
      "<rootDir>/src/*"
    ],
    "transform": {
      "\\.(t|j)sx?$": "@swc/jest"
    }
  },
  "devDependencies": {
    "@swc/core": "^1.3.100",
    "@swc/jest": "^0.2.29",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "bun-types": "^1.0.17",
    "cheerio": "1.0.0-rc.12",
    "cli-table": "^0.3.11",
    "jest": "30.0.0-alpha.2",
    "mitata": "^0.1.6",
    "prettier": "^3.1.1",
    "prettier-plugin-organize-imports": "^3.2.4",
    "pretty-ms": "^8.0.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "date-fns": "^2.30.0"
  }
}
