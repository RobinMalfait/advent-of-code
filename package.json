    "cli-table": "^0.3.11",
    "mitata": "^1.0.34",
    "prettier": "^3.7.3",
    "prettier-plugin-organize-imports": "^4.3.0",
    "pretty-ms": "^9.3.0",
    "typescript": "^5.9.3",
